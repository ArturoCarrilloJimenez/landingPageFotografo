{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/index.html", "/*.css", "/*.js", "/assets/**/*"]
      }
    },
    {
      "name": "images",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/images/**/*.webp", "/assets/images/**/*.jpg"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-images",
      "urls": ["/api/gallery/**"],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "30d",
        "strategy": "performance"
      }
    }
  ]
}
